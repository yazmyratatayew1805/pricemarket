{
    "name": "spatie/laravel-shop",
    "type": "project",
    "description": "Laravel event sourced shopping cart",
    "keywords": [
        "framework",
        "laravel",
        "event sourcing"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.7",
        "laravel/tinker": "^2.5",
        "spatie/browsershot": "^3.41",
        "spatie/enum": "^3.7",
        "spatie/laravel-enum": "^2.4",
        "spatie/laravel-event-sourcing": "^5.0-dev",
        "spatie/laravel-model-states": "^2.0",
        "spatie/period": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "brianium/paratest": "^6.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^2.18",
        "laravel/breeze": "^1.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ray": "^1.13",
        "spatie/ray": "^1.20",
        "vimeo/psalm": "^4.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "setup": [
            "@php artisan migrate:fresh --seed",
            "@php artisan ide-helper:models --nowrite"
        ],
        "test": "@php artisan test --parallel",
        "psalm": [
            "vendor/bin/psalm --threads=4 --no-diff"
        ],
        "ide-helper": [
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ]
    }
}
